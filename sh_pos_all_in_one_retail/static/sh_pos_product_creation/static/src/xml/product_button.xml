<?xml version="1.0" encoding="UTF-8"?>
<templates id="template" xml:space="preserve">

    <t t-name="TicketButton" t-inherit="point_of_sale.TicketButton" t-inherit-mode="extension">
        <xpath expr="//div[last()]" position="after">
            <button style="font-size:16px;" class="create_product" title="Create Product" t-on-click="() => this.trigger('click-product-button')"  > <i class="fa fa-plus"></i> </button>
        </xpath>
    </t>
</templates>
