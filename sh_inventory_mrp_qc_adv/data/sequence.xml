<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="name" model="ir.sequence">
        <field name="name">Quality Point Form</field>
        <field name="code">quality.point</field>
        <field name="prefix">QCP</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="quality_alert_seq" model="ir.sequence">
        <field name="name">Quality Alert Sequence</field>
        <field name="code">sh.quality.alert</field>
        <field name="prefix">QA</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="name_id" model="ir.sequence">
        <field name="name">quality team Form</field>
        <field name="code">quality.team</field>
        <field name="prefix">QT</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>

    <record id="quality_check_seq" model="ir.sequence">
        <field name="name">Quality Check Sequence</field>
        <field name="code">sh.quality.check.line</field>
        <field name="prefix">QC</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False" />
    </record>
    <record id="proforma_invoice_sequence" model="ir.sequence">
            <field name="name">Pro-Forma Invoice Sequence</field>
            <field name="code">proforma.invoice.sequence</field>
            <field name="prefix">PFI</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
    </record>
</odoo>